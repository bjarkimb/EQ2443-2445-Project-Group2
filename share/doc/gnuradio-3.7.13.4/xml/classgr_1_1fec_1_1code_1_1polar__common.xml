      <memberdef kind="function" id="classgr_1_1fec_1_1code_1_1polar__common_1add22e2355a7d6f5b806b5ae23d3f9ee7" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <type></type>
        <definition>gr::fec::code::polar_common::polar_common</definition>
        <argsstring>(int block_size, int num_info_bits, std::vector&lt; int &gt; frozen_bit_positions, std::vector&lt; char &gt; frozen_bit_values)</argsstring>
        <name>polar_common</name>
        <param>
          <type>int</type>
          <declname>block_size</declname>
        </param>
        <param>
          <type>int</type>
          <declname>num_info_bits</declname>
        </param>
        <param>
          <type>std::vector&lt; int &gt;</type>
          <declname>frozen_bit_positions</declname>
        </param>
        <param>
          <type>std::vector&lt; char &gt;</type>
