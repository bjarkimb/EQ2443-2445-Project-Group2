        <type></type>
        <definition>rpcbasic_register_set&lt; T, Tto &gt;::rpcbasic_register_set</definition>
        <argsstring>(const std::string &amp;block_alias, const char *functionbase, void(T::*function)(Tto), const pmt::pmt_t &amp;min, const pmt::pmt_t &amp;max, const pmt::pmt_t &amp;def, const char *units_=&quot;&quot;, const char *desc_=&quot;&quot;, priv_lvl_t minpriv_=RPC_PRIVLVL_MIN, DisplayType display_=DISPNULL)</argsstring>
