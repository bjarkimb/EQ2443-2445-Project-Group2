        <definition>virtual bool gr::digital::header_format_crc::parse</definition>
        <argsstring>(int nbits_in, const unsigned char *input, std::vector&lt; pmt::pmt_t &gt; &amp;info, int &amp;nbits_processed)</argsstring>
